{
  "name": "trends-analytics",
  "version": "1.0.0",
  "description": "Historical analytics with multi-series charts, volume overlays, moving averages, and volatility heatmaps",
  "main": "src/server/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/server/index.js",
    "dev": "nodemon src/server/index.js",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "db:migrate": "node src/server/database/migrate.js",
    "db:seed": "node src/server/database/seed.js",
    "build:frontend": "cd src/frontend && npm install && npm run build"
  },
  "keywords": [
    "analytics",
    "charts",
    "trends",
    "visualization"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "csv-stringify": "^6.4.5",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "helmet": "^7.0.0",
    "morgan": "^1.10.0",
    "pg": "^8.11.3",
    "pg-mem": "^3.0.5",
    "pngjs": "^7.0.0",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3"
  }
}
